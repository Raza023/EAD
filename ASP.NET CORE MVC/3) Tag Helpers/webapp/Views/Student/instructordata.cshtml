@{
    ViewData["Title"] = "Instructor Data";
}

@model List<webapp.Models.Instructor>

<h1>@ViewData["Title"]</h1>

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
</div>

<h2 class="text-center">Data for Instructors</h2>
<table border="2">
    <thead>
        <tr>
            <th>Name</th>
            <th>Subject</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Name</td>
                <td>@item.Subject</td>
            </tr>
        }
    </tbody>
</table>

<h1>@ViewBag.x</h1>
<h1>@ViewData["data"]</h1>